<body>
  <h1> Articles </h1>
  <%= link_to "Users", root_path%>
  <% @articles.each do |article| %>
      <p><%= link_to article.title, user_article_path(current_user.id, article.id) %> </p>
  <% end %>

  <%= link_to "New article", new_user_article_url(current_user.id) %>

  <% flash.each do |type, msg| %>
    <div class="alert alert-<%= type %> alert-dismissible">
      <%= msg %>
    </div>
  <% end %>
</body>
