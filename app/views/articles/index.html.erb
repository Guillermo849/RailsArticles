<body>
  <h1> Articles </h1>
  <%= link_to "Users", "users#index"%>
  <% @articles.each do |article| %>
      <p><%= link_to article.title, article_path(article.id) %> </p>
  <% end %>

  <%= link_to "New article", "articles/new"%>

  <% flash.each do |type, msg| %>
    <div class="alert alert-<%= type %> alert-dismissible">
      <%= msg %>
    </div>
  <% end %>
</body>
